{
    "extends": "eslint:recommended",
    "env": {
        "browser": true,
        "es6": true,
        "amd": true,
        "commonjs": true,
        "mocha": true
    },
    "ecmaFeatures": {
        "modules": true
    },
    "parserOptions": {
        "sourceType": "module"
    },
    "rules": {
        "no-unexpected-multiline": 2,
        "no-console" : "warn"
    },
    "globals": {
        "requirejs": true,
        "define": true,
        "_" : true,
        "$" : true
    }
}